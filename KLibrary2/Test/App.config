<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <sectionGroup name="keiho.configuration">
      <section name="singleTag1" type="System.Configuration.SingleTagSectionHandler" />
      <section name="dictionary1" type="System.Configuration.DictionarySectionHandler" />
    </sectionGroup>
  </configSections>

  <keiho.configuration>
    <singleTag1 isEnabled="true" count="99" />
    <dictionary1>
      <add key="Key1" value="1" />
      <add key="Key2" value="2" />
      <add key="Key3" value="3" />
    </dictionary1>
  </keiho.configuration>

  <appSettings>
    <add key="IsEnabled" value="true" />
    <add key="Count" value="99" />
    <add key="Since" value="2012-01-01 10:20:30" />
    <add key="Timeout" value="00:01:30" />
    <add key="DayOfWeek" value="Monday" />
  </appSettings>

  <system.net>
    <mailSettings>
      <smtp from="from@gmail.com">
        <network host="smtp.gmail.com" port="587" enableSsl="true" userName="from@gmail.com" password="password" />
      </smtp>
    </mailSettings>
  </system.net>

</configuration>
